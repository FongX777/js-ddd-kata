{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "error",
    "no-dupe-keys": "error",
    "no-dupe-class-members": "error",
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always"]
  }
}
